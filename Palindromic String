#include<stdio.h>
int main()
{
    char a[100];
    int l=0,i;
    scanf("%s",a);
    for(i=0;a[i]!='\0';i++)
    {
        l=l+1;
    }
    for(i=0;i<=(l-1)/2;i++)
    {
        if(a[i]!=a[l-1-i])
            {
            printf("NO");
            break;
            }
    }
    if(i==(l+1)/2)
        printf("YES");
    return 0;

}
